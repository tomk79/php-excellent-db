    } ,
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0" ,
        "phpoffice/phpexcel": "^1.8",
        "tomk79/filesystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "fabpot/goutte": "^3.2",
        "phpdocumentor/phpdocumentor": "^2.9"
    },
    "autoload": {
        "files": [
            "php/create.php",
            "php/caches.php",
            "php/parser/xlsx.php",
            "php/migrate/init_tables.php",
            "php/dba/crud.php",
            "php/endpoint/form.php",
            "php/endpoint/rest.php"
        ]
    }
}
